//[202] chrissstrahl - upgraded to reuse previousely spawned markers and scale 2.5
//=========================================================================
spawnDebug local.origin:{
//=========================================================================
if(level.cMTE_coop_spawnlocations){if(!level.cMTE){level.cMTE=0}; level.cMTE++; println( "-#-#- thread spawnlocations/spawnDebug->"+level.cMTE+"" )} //[202] chrissstrahl - this is to debug the coop mod thread handling
	if!( level.coop_debugSpawn ){ end }
	if(level.coop_spawnDebugNum == NIL || level.coop_spawnDebugNum>8){
		level.coop_spawnDebugNum=1
	}	
	
	local.debugMarker = $("coop_spawnDebugMerker"+level.coop_spawnDebugNum)
	if(local.debugMarker){
		local.debugMarker origin (local.origin)
	}else{
		spawn script_model model "models/items/Af_P_Lantern.tik" origin (local.origin) notsolid 1 targetname ("coop_spawnDebugMerker"+level.coop_spawnDebugNum) scale 2.5
	}
	level.coop_spawnDebugNum++
}end

//=========================================================================
placeSpawn local.origin local.angle:{
//=========================================================================
if(level.cMTE_coop_spawnlocations){if(!level.cMTE){level.cMTE=0}; level.cMTE++; println( "-#-#- thread spawnlocations/placeSpawn->"+level.cMTE+"" )} //[202] chrissstrahl - this is to debug the coop mod thread handling
	if( level.coopSpawnLocationCurrentNumber == NIL){
		level.coopSpawnLocationCurrentNumber = 0
	}
	
	if( level.coopSpawnLocationCurrentNumber > 7){
		iprintlnbold_noloc ( "MAXIMUM NUMBER OF COOP SPAWNPOINTS REACHED" )
		end
	}
	level.coopSpawnLocationCurrentNumber++;
	local.targetname = "coopPlayerSpawn"
	local.targetname += level.coopSpawnLocationCurrentNumber	
	spawn PlayerAlliedDeathmatchStart origin local.origin angle local.angle targetname local.targetname
	//spawn PlayerDeathmatchStart origin local.origin angle local.angle targetname local.targetname
}end


//=========================================================================
updateSpawn local.origin local.angle:{
//=========================================================================
if(level.cMTE_coop_spawnlocations){if(!level.cMTE){level.cMTE=0}; level.cMTE++; println( "-#-#- thread spawnlocations/updateSpawn->"+level.cMTE+"" )} //[202] chrissstrahl - this is to debug the coop mod thread handling
	if( level.coopSpawnLocationCurrentNumber > 7){
		level.coopSpawnLocationCurrentNumber = 0
	}
	level.coopSpawnLocationCurrentNumber++;
	$( "coopPlayerSpawn" + level.coopSpawnLocationCurrentNumber ) origin local.origin
	$( "coopPlayerSpawn" + level.coopSpawnLocationCurrentNumber ) angle local.angle
}end

